<?xml version="1.0" encoding="windows-1252" ?>
<adfc-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.2">
  <task-flow-definition id="main-flow">
    <default-activity>MainView</default-activity>
    <data-control-scope>
      <isolated/>
    </data-control-scope>
    <managed-bean id="__1">
      <managed-bean-name>employeeBean</managed-bean-name>
      <managed-bean-class>blog.anirbanm.keyboard.view.bean.EmployeeManager</managed-bean-class>
      <managed-bean-scope>pageFlow</managed-bean-scope>
    </managed-bean>
    <view id="MainView">
      <page>/WEB-INF/MainView.jsff</page>
    </view>
    <use-page-fragments/>
  </task-flow-definition>
</adfc-config>
